{
    "name": "adrenalinkin/doctrine-naming-strategy",
    "description": "Custom Doctrine ORM naming strategy",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "doctrine",
        "doctrine-naming-strategy",
        "doctrine-orm"
    ],

    "authors": [
        {
            "email": "adrenalinkin@gmail.com",
            "name": "Viktor Linkin"
        }
    ],

    "require": {
        "php": "^7.1||^8.0",
        "doctrine/orm": "^2.3"
    },

    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^5.7||^6.0||^7.0||^8.0||^9.0"
    },

    "autoload": {
        "psr-4": {
            "Linkin\\Component\\DoctrineNamingStrategy\\": ""
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },

    "autoload-dev": {
        "psr-4": {
            "Linkin\\Component\\DoctrineNamingStrategy\\Tests\\": "Tests/"
        }
    },

    "config": {
        "sort-packages": true
    }
}
